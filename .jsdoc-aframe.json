{
    "source": {
        "include": "src/components/"
    },
    "opts": {
        "recurse": true,
        "destination": "doc/aframe",
        "readme": "README.template.md",
        "template": "./node_modules/minami"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "default": {
            "outputSourceFiles": false,
            "layoutFile": "./node_modules/minami/tmpl/layout.tmpl"
        }
    }
}
